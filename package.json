{
	"name": "postgresql-obsidian",
	"version": "0.0.1",
	"description": "An obsidian.md plugin that sends your data to a PostgreSQL database for further analysis.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"ncu": "npx npm-check-updates",
		"lint": "eslint ./src"
	},
	"keywords": [
		"obsidian",
		"obsidian.md",
		"postgresql",
		"postgres",
		"database",
		"export"
	],
	"author": "clouedoc",
	"license": "MIT",
	"devDependencies": {
		"@rushstack/eslint-config": "^2.6.1",
		"@types/node": "^16.11.6",
		"@types/pg": "^8.6.5",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "0.14.47",
		"eslint": "^8.18.0",
		"obsidian": "^0.15.2",
		"obsidian-dataview": "^0.5.36",
		"pg": "^8.7.3",
		"pg-native": "^3.0.0",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
	}
}
